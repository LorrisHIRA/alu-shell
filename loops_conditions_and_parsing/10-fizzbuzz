#!/usr/bin/env bash
# Display numbers 1-100 with Fizz, Buzz, or FizzBuzz

for i in {1..100}; do
    if [ $((i % 15)) -eq 0 ]; then
        echo "FizzBuzz"
    elif [ $((i % 3)) -eq 0 ]; then
        echo "Fizz"
    elif [ $((i % 5)) -eq 0 ]; then
        echo "Buzz"
    else
        echo "$i"
    fi
done

